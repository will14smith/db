CREATE TABLE
 === Plan ===                                 
 projection (*)                               
   filter (name == 'Will' OR country == 'GB') 
     scan (tbl)                               
                                              
 === Program ===                              
 [                                            
 	cursor                                      
 ] {                                          
 	CUR.OPEN.RT tbl                             
 	CUR.FIRST                                   
 	STORE S0_cursor                             
 L1_loop_start:                               
 	LOAD S0_cursor                              
 	CUR.NEXT L2_loop_done                       
 	STORE S0_cursor                             
 	LOAD S0_cursor                              
 	COL 1                                       
 	CONST.S Will                                
 	J.NOTEQUAL L3_or_lhs_false                  
 	J L4_or_lhs_true                            
 L3_or_lhs_false:                             
 	LOAD S0_cursor                              
 	COL 2                                       
 	CONST.S GB                                  
 	J.NOTEQUAL L1_loop_start                    
 L4_or_lhs_true:                              
 	LOAD S0_cursor                              
 	COL 0                                       
 	LOAD S0_cursor                              
 	COL 1                                       
 	LOAD S0_cursor                              
 	COL 2                                       
 	MK.ROW 3                                    
 	YIELD                                       
 	J L1_loop_start                             
 L2_loop_done:                                
 	FINISH                                      
 }                                            
